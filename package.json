{
  "name": "stream-compare",
  "version": "0.1.0",
  "description": "Compare the behavior of readable streams.",
  "keywords": [
    "assert",
    "compare",
    "stream",
    "streams",
    "test"
  ],
  "license": "MIT",
  "homepage": "https://github.com/kevinoid/stream-compare",
  "bugs": "https://github.com/kevinoid/stream-compare/issues",
  "author": "Kevin Locke <kevin@kevinlocke.name>",
  "repository": {
    "type": "git",
    "url": "https://github.com/kevinoid/inflate-auto.git"
  },
  "main": "index.js",
  "files": [
    "LICENSE.txt",
    "README.md",
    "index.js",
    "lib"
  ],
  "scripts": {
    "lint": "eslint . && echo Lint passed",
    "postversion": "git push --tags origin master && echo \"Remember to update GitHub Releases from CHANGELOG.md\n(Until skywinder/github-changelog-generator#56 is fixed.)\"",
    "preversion": "./scripts/preversion.js",
    "test": "npm run lint && npm run test-ci",
    "test-ci": "mocha --recursive test/",
    "test-cov": "istanbul cover _mocha --recursive test/",
    "version": "set -a && . \"${XDG_CONFIG_HOME:-$HOME/.config}/github_changelog_generator\" && github_changelog_generator"
  },
  "dependencies": {
    "debug": "^2.2.0",
    "object-assign": "^4.0.1"
  },
  "devDependencies": {
    "bluebird": "^3.1.5",
    "buffer-equal": "^1.0.0",
    "codecov": "^1.0.1",
    "coveralls": "^2.11.6",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^4.0.0",
    "istanbul": "^0.4.1",
    "mocha": "^2.3.4",
    "pify": "^2.3.0",
    "request": "^2.69.0",
    "should": "^8.0.2",
    "which": "^1.2.4"
  },
  "engines": {
    "node": ">=0.10"
  }
}
